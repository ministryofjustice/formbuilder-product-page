<script>
  if(location.hostname == 'moj-forms.service.justice.gov.uk') {
    mojforms.analytics.setGoogleMeasurementId('<%= config.analytics_live %>');
  }
  else {
    mojforms.analytics.setGoogleMeasurementId('<%= config.analytics_staging %>');
  }

  if(mojforms.cookiepolicy.analytics()) {
    mojforms.analytics.add();
  }
  else {
    mojforms.analytics.remove();
  }
</script>

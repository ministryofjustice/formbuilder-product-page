<script>
  function injectGoogleAnalytics() {
    // Start: Google Analytics
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

    ga('create', '<%= config.analytics %>', 'auto');
    ga('set', 'anonymizeIp', true);
    ga('set', 'displayFeaturesTask', null);
    ga('set', 'transport', 'beacon');
    ga('send', 'pageview');
    // End: Google Analytics
  }

  function rejectGoogleAnalytics() {
    window.ga = function() { /* disabled */ }
    window.gtag = function() { /* disabled */ }
    window.GoogleAnalyticsObject = "";
    window.dataLayer = {};
    window['ga-disable-' + '<%= config.analytics %>'] = true;
  }

  // If we already know the user has accepted, run the Google Analytics code.
  // If they only accept (with Cookie banner) after this point, we'll call it
  // with the moj.cookiepolicy.accept() function instead.
  if(moj.cookiepolicy.analyticsAllowed()) {
    console.log("TEMP DEV ONLY: google loading...");
    injectGoogleAnalytics();
  }
  else {
    rejectGoogleAnalytics();
    console.log("TEMP DEV ONLY: google denied!!");
  }
</script>
